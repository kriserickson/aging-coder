---
pagination:
  data: collections.posts
  size: 1
  alias: post
  addAllPagesToCollections: false
eleventyExcludeFromCollections: true
---
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <!-- Canonical tag is crucial here so Google ignores this duplicate -->
    <link rel="canonical" href="{{ post.url | url }}">

    <!-- Meta refresh redirect -->
    <meta http-equiv="refresh" content="0; url={{ post.url | url }}">

    <title>Redirecting to {{ post.data.title }}</title>
</head>
<body>
<p>This page has moved. <a href="{{ post.url | url }}">Click here if you are not redirected.</a></p>
</body>
</html>
